        <table border="1" width="80%">
            <th>HTML TAGS</th>
            <th>Output</th>
            <tr>
                <td>Normal text</td>
                <td>Hello world</td>
            </tr>
            <tr>
                <td>Font & it's attribute</td>
                <td><font size="+3"><font color="blue"><b>Hello World</font></b></font></td>
            </tr>
            <tr>
                <td>B tag</td>
                <td><b>Bold</b></td>
            </tr>
            <tr>
            <td>I tag</td>
            <td><i>Italic</i></td>
            </tr>
            <tr>
                <td>U tag</td>
                <td><u>underline</u></td>
            </tr>
            <tr>
                <td>strong tag</td>
                <td><strong>Strong</strong></td>
            </tr>
            <tr>
                <td>strike tag</td>
                <td><strike>strike</strike></td>
            </tr>
            <tr>
                <td>small tag</td>
                <td><small>Text in small font</small></td>
            </tr>
            <tr>
                <td>Big tag</td>
                <td><big>Text in big font</big></td>
            </tr> 
            <tr>
                <td>Subscript tag</td>
                <td><b>X</b><sub>2</sub></td>
            </tr>
            <tr>
                <td>Superscript tag</td>
                <td><b>X</b><sup>2s</sup></td>
            </tr>  
        </table>
